<?xml version="1.0" encoding="utf-8"?>
<xar:template xmlns:xar="http://xaraya.com/2004/blocklayout">
    <xar:if condition="$layout eq 'monthyear'">
        <xar:template file="monthyeartemplate-output"/>
    <xar:elseif condition="$layout eq 'daymonth'"/>
        <xar:template file="daymonthtemplate-output"/>
    <xar:else/>
        <xar:template file="datetemplate-output"/>
    </xar:if>
</xar:template>